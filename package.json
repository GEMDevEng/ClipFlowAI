  },
  "homepage": "https://gemdeveng.github.io/ClipFlowAI",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "gh-pages": "^6.0.0",
    "jest": "^29.6.4",
    "supertest": "^7.1.0"
